{
    "name": "@medly-components/core",
    "version": "1.2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "scripts": {
        "typecheck": "tsc -p tsconfig.json",
        "build:js": "cross-env BABEL_ENV=production babel --root-mode upward src --out-dir dist --extensions .ts,.tsx --no-comments",
        "build:types": "tsc -p tsconfig.build.json",
        "prebuild": "yarn typecheck",
        "build": "yarn build:js --delete-dir-on-start ",
        "postbuild": "yarn build:types",
        "watch": "run-p \"build:types -w\" \"build:js --watch\"",
        "s3upload": "s3cmd put medly-components-core-* s3://medly-artifacts/medly-components/",
        "ci_upload": "aws s3 cp . s3://medly-artifacts/medly-components/ --recursive --exclude '*' --include 'medly-components-*' --acl public-read"
    },
    "peerDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "styled-components": "^4.2.0"
    },
    "dependencies": {
        "@datepicker-react/styled": "^1.7.1",
        "@medly-components/icons": "^1.2.0",
        "@medly-components/theme": "^1.2.0",
        "@medly-components/utils": "^1.2.0",
        "shortid": "^2.2.14"
    },
    "devDependencies": {
        "@types/react-datepicker": "^2.8.1",
        "@types/shortid": "^0.0.29"
    }
}
