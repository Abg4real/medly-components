{
    "name": "@medly-components/theme",
    "version": "1.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "scripts": {
        "prebuild": "tsc -p tsconfig.json",
        "build": "cross-env BABEL_ENV=production babel --root-mode upward src --out-dir dist --extensions .ts,.tsx --delete-dir-on-start --no-comments",
        "postbuild": "tsc -p tsconfig.build.json",
        "s3upload": "s3cmd put medly-components-theme-1.1.0.tgz s3://medly-artifacts/medly-components/"
    }
}
